   "source": [
    "def CountTrailingZeros(n):\n",
    "    result = 0\n",
    "    i = 5\n",
    "    while (n / i>= 1):\n",
    "        result += int(n / i)\n",
    "        i *= 5\n",
    "    return result"
   ]
