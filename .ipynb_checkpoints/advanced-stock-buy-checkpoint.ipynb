   "source": [
    "def MaxProfit(price):\n",
    "    n = len(price)\n",
    "    profit = 0\n",
    "    for i in range(0, n):\n",
    "        if price[i] > price[i-1]:\n",
    "            profit += (price[i]-price[i-1])\n",
    "    return profit"
   ]
