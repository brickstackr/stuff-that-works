   "source": [
    "def get_recursive_factorial(n):\n",
    "    if n < 0:\n",
    "        return -1\n",
    "    elif n < 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * get_recursive_factorial(n-1)"
   ]
  },
