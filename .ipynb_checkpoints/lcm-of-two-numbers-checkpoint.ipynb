   "source": [
    "#optimized euclidean algorithm\n",
    "def gcd(a,b):\n",
    "    if b ==0:\n",
    "        return a\n",
    "    else:\n",
    "        return gcd(b, a%b)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "b682f07a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def lcm(a,b):\n",
    "    return(a*b)/gcd(a,b)"
   ]
  },
